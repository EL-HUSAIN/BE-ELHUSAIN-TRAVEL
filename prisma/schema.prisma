generator client {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model VisaApplication {
  id             Int      @id @default(autoincrement())
  name           String
  passportNumber String
  nationality    String
  status         String   @default("PENDING")
  createdAt      DateTime @default(now())
}